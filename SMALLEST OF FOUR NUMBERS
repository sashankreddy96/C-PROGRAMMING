#include <stdio.h>

int main()
{
    int a, b, c, d;
    printf("enter a, b, c & d values: ");
    scanf("%d %d %d %d", &a, &b, &c, &d);

    if (a < b && a < c && a < d)
        printf("a is Small
");
    else if (b < c && b < d && b < a)
        printf("b is Small
");
    else if (c < d && c < a && c < b)
        printf("c is Small
");
    else
        printf("d is Small
");

    return 0;
}
